{
  "name": "chisai",
  "version": "0.3.0",
  "description": "A library for building micro stores",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosephLenton/maja.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "josephlenton@gmail.com",
  "license": "MIT",
  "typings": "./lib/index.d.ts",
  "source": "./src/index.ts",
  "main": "./lib/index.js",
  "files": [
    "lib/**/*"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:src",
    "build:src": "rollup --config ./rollup.config.ts --configPlugin typescript",
    "clean": "yarn run clean:build",
    "clean:build": "rimraf ./lib/*",
    "test": "yarn run jest",
    "deploy": "yarn run build && yarn publish --access public"
  },
  "dependencies": {},
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.4.0",
    "@types/react": "^17.0.35",
    "babel-jest": "^27.4.6",
    "babel-loader": "^8.2.3",
    "jest": "^27.4.7",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.60.0",
    "ts-jest": "^27.1.3",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "bugs": {
    "url": "https://github.com/JosephLenton/maja/issues"
  },
  "homepage": "https://github.com/JosephLenton/maja#readme",
  "keywords": [
    "store"
  ]
}
